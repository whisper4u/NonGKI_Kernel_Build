CONFIG_ENV=Profiles/opd.env

DEVICE_NAME=oppo_pad
DEVICE_CODENAME=oppo_pad

CUSTOM_CMDS:CLANG_TRIPLE=aarch64-linux-gnu-
EXTRA_CMDS:LD=ld.lld

KERNEL_SOURCE=https://github.com/oppo-source/android_kernel_oppo_sm8250.git
